 var moment1 = moment()
        var moment2 = moment() 
        /*__seta o mês e o anos da data do anoversário do cliente*/
        moment2.months(month) 
        moment2.years(yearNas)
        //console.log(moment1.format('DD/MM/YYYY'),"atual ");
